<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.littlepage.mapper.CustomerlogMapper">
	 <insert id="add">
	 	insert customerlog values(#{id},#{info})
	 </insert>
	 <select id="getAllBuyId" resultType="com.littlepage.entity.Customerlog">
	 	select * from customerlog where id=#{id}
	 </select>
</mapper>
